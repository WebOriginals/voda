                    .filter__input.filter-input
                        .filter-input__wr
                            .filter-input__title.filter-title Дата и время заявки
                            .filter-input__body.filter-input__body-data
                                span c
                                input(type="date"  name="trip-start" value="2021-07-22" min="2021-01-01" max="2030-12-31").filter-input__input
                    .filter__input.filter-input
                        .filter-input__wr
                            .filter-input__body.filter-input__body-data
                                input(type="time"  name="trip-finish" value="2021-07-22" min="2021-01-01" max="2030-12-31").filter-input__input
                .chiefRegistry__date 
                    .filter__input.filter-input
